{
  "name": "joshdavidmiller.com",
  "version": "0.0.1",
  "description": "The public website of Josh David Miller",
  "main": "server.js",
  "scripts": {
    "prestart": "npm run build",
    "dev": "npm run watch && npm start",
    "build-js": "browserify -d -t babelify src/app.js -o dist/app.js",
    "build-css": "node ./node_modules/.bin/node-sass --include-path node_modules --source-map-embed src/app.scss dist/app.css",
    "build": "npm run build-js && npm run build-css",
    "watch-js": "watchify -d -t babelify src/app.js -o dist/app.js &",
    "watch-css": "node-sass -r -w --source-map-embed src/app.scss dist/app.css &",
    "watch": "npm run watch-js && npm run watch-css",
    "postwatch": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "github.com/joshdmiller/joshdavidmiller.com"
  },
  "author": "Josh David Miller <josh@joshdmiller.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshdmiller/joshdavidmiller.com/issues"
  },
  "homepage": "https://github.com/joshdmiller/joshdavidmiller.com",
  "devDependencies": {
    "angular-material": "git://github.com/angular/material#v0.8.3",
    "babelify": "^5.0.4",
    "browserify": "^9.0.3",
    "node-sass": "^3.0.0-pre",
    "react": "^0.13.0",
    "watchify": "^2.4.0"
  },
  "dependencies": {
    "express": "^4.12.2"
  }
}
