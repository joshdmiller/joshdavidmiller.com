{
  "name": "joshdavidmiller.com",
  "version": "1.0.0",
  "description": "The source code of joshdavidmiller.com",
  "scripts": {
    "start": "NODE_ENV=development nodemon server.js",
    "dev": "NODE_ENV=development node webpack.server.js",
    "test": "tape -r babel-register 'src/**/*spec.js' | faucet",
    "cover": "nyc -x '**/*spec.js' -r text -r html -r lcov npm test",
    "lint": "eslint src"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshdmiller/joshdavidmiller.com.git"
  },
  "author": "Josh David Miller <josh@joshdavidmiller.com> (http://joshdavidmiller.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshdmiller/joshdavidmiller.com/issues"
  },
  "homepage": "https://github.com/joshdmiller/joshdavidmiller.com",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "enzyme": "^2.3.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "faucet": "0.0.1",
    "nyc": "^7.0.0",
    "react-addons-test-utils": "^15.2.0",
    "react-hot-loader": "^3.0.0-beta.1",
    "tape": "^4.6.0",
    "webpack": "^2.1.0-beta.14",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.7.1",
    "express": "^5.0.0-alpha.2",
    "nodemon": "^1.9.2",
    "radium": "^0.18.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.5.2",
    "react-tap-event-plugin": "^1.0.0"
  }
